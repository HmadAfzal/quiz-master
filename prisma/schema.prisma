// This goes in your schema.prisma file
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}


model User {
  id        String   @id @default(cuid())
  email     String   @unique
  username  String   @unique
  password  String
  pfpUrl    String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

